{
  "expo": {
    "name": "Nissu",
    "slug": "nissu",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "scheme": "nissu",
    "newArchEnabled": true,
    "plugins": ["expo-router", "expo-asset", "expo-audio"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E9E8DF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.annahilla.nissu",
      "versionCode": 9
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d6d06ea2-b0ce-4f80-887c-1d7d2d5108ad"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d6d06ea2-b0ce-4f80-887c-1d7d2d5108ad"
    }
  }
}
